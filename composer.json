{
    "name": "fractas/htmleditorfield-content-templates",
    "description": "SilverStripe module for creation and organization predefined content templates for your Html Editor Fields",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/fractaslabs/silverstripe-htmleditorfield-content-templates/",
    "keywords": ["silverstripe", "content", "tinymce", "content template", "HtmlEditorField-content-templates"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/fractaslabs/silverstripe-htmleditorfield-content-templates/issues"
    },
    "authors": [{
        "name": "Milan Jelicanin",
        "email": "milan.jelicanin@fractas.com",
        "homepage": "https://www.fractas.com/"
    }, {
        "name": "Petar Simic",
        "email": "petar.simic@fractas.com",
        "homepage": "https://www.fractas.com/"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/cms": "^4.0",
        "silverstripe/framework": "^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Fractas\\HtmlContentTemplate\\": "src/"
        }
    }
}
